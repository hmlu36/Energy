                    <script type="text/javascript">
                        $('#YearOrMonth').change(function(){
                            refreshDDL();
                        });
                        $(function () {
                            refreshDDL();
                        });
                        function refreshDDL() {
                            if ($('#YearOrMonth').val() == 'Y') {
                                $('#ddl_s_y').prop('disabled', false);
                                $('#ddl_s_y').css('display', '');
                                $('#ddl_e_y').prop('disabled', false);
                                $('#ddl_e_y').css('display', '');

                                $('#ddl_s_m').prop('disabled', true);
                                $('#ddl_s_m').css('display', 'none');
                                $('#ddl_e_m').prop('disabled', true);
                                $('#ddl_e_m').css('display', 'none');
                            } else if ($('#YearOrMonth').val() == 'M') {
                                $('#ddl_s_y').prop('disabled', true);
                                $('#ddl_s_y').css('display', 'none');
                                $('#ddl_e_y').prop('disabled', true);
                                $('#ddl_e_y').css('display', 'none');

                                $('#ddl_s_m').prop('disabled', false);
                                $('#ddl_s_m').css('display', '');
                                $('#ddl_e_m').prop('disabled', false);
                                $('#ddl_e_m').css('display', '');
                            };
                        }
                    </script>
                    民國
                    <div class="select">
                        <i class="icon ion-ios-arrow-down"></i>
                        <select id="ddl_s_m" name="Start">
                            @for (int i = 0; i <= _TOTALMONTH; i++)
                            {
                                DateTime r1 = _START.AddMonths(i);
                                string s1 = r1.ToString("yyyMM", culture).PadLeft(5, '0');
                                string s1Display = r1.ToString("yyy年MM月", culture);
                                <option @(s1 == Model.Start ? "selected" : "") value="@s1">@s1Display</option>
                            }
                        </select>
                        <select id="ddl_s_y" name="Start" disabled style="display:none;"> 
                            @for (int i = 0; i <= _TOTALYEARS; i++)
                            {
                                DateTime r1 = _YEAR_START.AddYears(i);
                                string display = r1.ToString("yyy年", culture);
                                string s1 = r1.ToString("yyyMM", culture).PadLeft(5, '0');
                                <option @(s1.Substring(0, 3) == Model.Start.Substring(0, 3) ? "selected" : "") value="@s1">@display</option>
                            }
                        </select>
